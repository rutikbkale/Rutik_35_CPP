#include<iostream.h>
#include<conio.h>
class A
{
public:
void fun()
{
cout<<"Function with no arguments";
}
void fun(int x)
{
cout<<"x is: "<<x;
cout<<"Function with arguments";
}
void fun(double y)
{
cout<<"y is: "<<y;
cout<<"function with double argument";
}};
int main()
{
A obj;
obj.fun();
obj.fun(5);
obj.fun(3.5);
return 0;
}